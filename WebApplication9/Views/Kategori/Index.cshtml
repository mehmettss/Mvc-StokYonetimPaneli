
@using WebApplication9.Models.Entity

@model List<TBLKATEGORILER> 


@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>KATEGORI ID</th>
        <th>KATEGORI ADI</th>
        <th>SİL</th>
        <th>GUNCELLE</th>
        
      
    </tr>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th>
                    @item.KATEGORIID
                </th>
                <td>
                    @item.KATEGORIAD
                </td>
                <td><a href="/Kategori/Sil/@item.KATEGORIID" class="btn btn-danger">SİL</a></td>
                <td><a href="/Kategori/KategoriGetir/@item.KATEGORIID" class="btn btn-success">GUNCELLE</a></td>
                
            </tr>
        }
    </tbody>
</table>
<a href="/Kategori/Yenikategori/" class="btn btn-primary">YENİ KATEGORİ EKLE</a>
